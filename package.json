{
  "name": "text-formatter-addin",
  "version": "1.0.0",
  "description": "A Word Add-in that formats selected text with Times New Roman font and bold styling.",
  "main": "src/taskpane/taskpane.js",
  "scripts": {
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "build": "webpack --mode production",
    "dev-server": "webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "validate": "office-addin-manifest validate manifest.xml",
    "update-urls": "node update-manifest-urls.js"
  },
  "keywords": [
    "word",
    "office",
    "add-in",
    "formatter"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "office-addin-debugging": "^5.0.12",
    "office-addin-dev-certs": "^1.11.4",
    "office-addin-lint": "^2.2.6",
    "office-addin-manifest": "^1.12.7",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {}
}


